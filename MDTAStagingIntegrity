#AMSAddressStandardizationTask
$filePath = '$(System.ArtifactsDirectory)\tfiles\Integrity.sprint.AMSAddressStandardizationTask\tcore.AMSAddressStandardizationTask.exe.config'
$emailFromAddress = ''
$xml = [xml](Get-Content $filePath)
$targetNode = $xml.SelectSingleNode('/configuration/appSettings/add[@key="emailFromAddress"]')
$targetNode.SetAttribute('value',"$emailFromAddress")
$xml.Save($filePath)
$emailToAddress = ''
$xml = [xml](Get-Content $filePath)
$targetNode = $xml.SelectSingleNode('/configuration/appSettings/add[@key="emailToAddress"]')
$targetNode.SetAttribute('value',"$emailToAddress")
$xml.Save($filePath)

